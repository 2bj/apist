{
    "name": "sleeping-owl/apist",
    "description": "Package to provide api-like access to sites without api",
	"license": "MIT",
	"keywords": ["api", "crawler"],
    "authors": [
        {
            "name": "Sleeping Owl",
            "email": "owl.sleeping@yahoo.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*",
		"guzzlehttp/guzzle": "5.*",
		"symfony/css-selector": "2.5.*",
		"symfony/dom-crawler": "2.5.*"
    },
	"require-dev": {
		"phpunit/phpunit": "4.*",
		"mockery/mockery": "0.*"
	},
    "autoload": {
        "psr-0": {
            "SleepingOwl\\Apist": "src/"
        }
    },
    "minimum-stability": "stable"
}
